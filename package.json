{
  "name": "emojo-lookup",
  "version": "0.0.2",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "fastify start -w -l info -P app.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "fastify start -l info app.js",
    "test": "tap \"test/**/*.test.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/sensible": "^6.0.3",
    "@fastify/static": "^8.2.0",
    "@fastify/view": "^11.1.1",
    "ejs": "^3.1.10",
    "fastify": "^5.6.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.1",
    "milligram": "^1.4.1",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "fetch-mock": "npm:@gr2m/fetch-mock@9.11.0-pull-request-644.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tap": "^21.1.0"
  },
  "tap": {
    "check-coverage": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
